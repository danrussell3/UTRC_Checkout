@model  IEnumerable<Check_Out_App_ULC.Models.ImportUser>

@{
    ViewBag.Title = "FindStudent";
}
@using (Html.BeginForm())
{
    <input type="text" name="FIRST_NAME" />
        <input type="text" name="LAST_NAME" />
        <input type="text" name="ENAME" />
        <input type="submit" />
}
<h2>FindStudent</h2>

@if (Model != null)
{
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LAST_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FIRST_NAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CSU_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ENAME)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FIRST_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LAST_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CSU_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ENAME)
                </td>
                <td>
                    @Html.ActionLink("Import", "Import", new { id = item.CSU_ID }) |

                </td>
            </tr>
        }

    </table>
}
